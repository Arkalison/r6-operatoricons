{
    "name": "r6-operatoricons",
    "version": "2.0.0",
    "description": "r6-operatoricons is a collection of Rainbow Six: Siege operator icons available in SVG, PNG and AI. Each icon is made by hand in Adobe Illustrator and is free to download under the Creative Commons Attribution 4.0 license.",
    "repository": "https://github.com/MarcoPixel/r6-operatoricons",
    "license": "CC-BY-4.0",
    "author": "Marco Vockner <marcopixel@live.de>",
    "contributors": [
        "dtSniper (https://twitter.com/sniperdt)",
        "Joey Foo (https://github.com/joeyfoo)"
    ],
    "main": "index.js",
    "scripts": {
        "build:png": "ts-node ./scripts/processPNG.ts",
        "build:zip": "ts-node ./scripts/processZIP.ts",
        "build:package": "ts-node ./scripts/processPACKAGE.ts",
        "lint": "eslint --ignore-path .gitignore . --ext .ts,.tsx,.js,.jsx",
        "lint:fix": "yarn lint --fix",
        "type-check": "tsc --noEmit"
    },
    "prettier": {
        "printWidth": 120,
        "singleQuote": false,
        "tabWidth": 4,
        "useTabs": false
    },
    "devDependencies": {
        "@types/archiver": "^3.0.0",
        "@types/glob": "^7.1.1",
        "@types/node": "^12.7.2",
        "@types/sharp": "^0.22.2",
        "@typescript-eslint/eslint-plugin": "2.0.0",
        "@typescript-eslint/parser": "^2.0.0",
        "archiver": "^3.1.1",
        "eslint": "6.2.1",
        "eslint-config-airbnb-typescript": "^4.0.1",
        "eslint-config-prettier": "^6.1.0",
        "eslint-plugin-import": "2.18.2",
        "eslint-plugin-jsx-a11y": "6.2.3",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "7.14.3",
        "eslint-plugin-react-hooks": "2.0.1",
        "eslint-plugin-unicorn": "^10.0.0",
        "glob": "^7.1.4",
        "ora": "^3.4.0",
        "prettier": "^1.18.2",
        "sharp": "^0.23.0",
        "svgo": "^1.3.0",
        "ts-node": "^8.3.0",
        "typescript": "^3.5.3"
    }
}
